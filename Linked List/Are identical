    //Function to check whether two linked lists are identical or not.
    areIdentical(head1, head2)
    {
        //your code here
        while(head1 && head2){
            if(head1.data === head2.data)
                {
                    head1 = head1.next;
                    head2= head2.next;
                }
            else
                return false;
        }
        return true;
    }
