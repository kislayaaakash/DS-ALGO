class Node{
    constructor(val){
        this.value = val;
        this.next = null;
    }
}

class SingleList{
    constructor(){
        this.head = null;
        this.tail = null;
        this.size = 0;
    }
    
    push(value){
        var newNode = new Node(value)
        if(!this.head)
            this.head = newNode;
        else{
            var current = this.head;
            while(current.next)
                current = current.next;
            current.next = newNode;
        }
        this.size++;
   }         
}
