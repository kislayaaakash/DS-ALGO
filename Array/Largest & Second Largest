 largestAndSecondLargest(sizeOfArray, arr)
    {
        var max = arr[0];
        var max2 = -1;
        var arr2 = [];
        for(var i =0; i < sizeOfArray; i++){
            if(arr[i] > max){
                max2 = max;
                max= arr[i];
            }
            if(arr[i] > max2 && arr[i] != max)
                max2 = arr[i]
        }
    var arr2 = [max,max2];
    return arr2;
}
