    // Function to find maximum for every adjacent pairs in the array.
    maximumAdjacent(sizeOfArray, arr){
        var temp = new Array(sizeOfArray - 1);
        var max = -1;
        for(var i =0; i <sizeOfArray -1;i++){
            max = Math.max(arr[i],arr[i+1]);
            temp[i] = max;
        }
        return temp;
    }
